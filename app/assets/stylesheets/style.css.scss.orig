    width: 40%;
}
.wallet-ps-list-box .line .item{
    line-height: 42px;
}
.wallet-ps-list-box .line{
    overflow: hidden;
    cursor: pointer;
    border-bottom: 1px solid #f4f3fa;
}
.wallet-ps-list-box .line:hover{
    background: #fcfcfc;
}
.wallet-ps-list-box .line.act{
    position: relative;
    border-top: 1px solid #e5e5e5;
    border-bottom: 1px solid #e5e5e5;
    border-right: 1px solid #fff;
    margin-right: -1px;
    border-top-left-radius: 5px;
    border-bottom-left-radius: 5px;
    margin-top: -1px;
    cursor: initial;
}
.wallet-ps-list-box .line.act:before{
    display: block;
    content: '';
    position: absolute;
    width: 3px;
    top: 0;
    left: 0;
    bottom: 0;
    background: #282e7a;
}
.wallet-ps-list-box .line.act:hover{
    background: #fff;
}
.wallet-ps-list-box .name{
    line-height: 16px;
