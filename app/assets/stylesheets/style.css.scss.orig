.top-user-box .menu{
    display: none;
    position: absolute;
    width: 200px;
    top: 55px;
    right: -5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    background: #22275e;
    z-index: 10;
    -webkit-box-shadow: 0 -5px 6px rgba(0,0,0,0.2);
    -moz-box-shadow:  0 -5px 6px rgba(0,0,0,0.2);
    box-shadow:  0 -5px 6px rgba(0,0,0,0.2);
    padding: 5px 0;
}
.top-user-box .menu::after {
    content: '';
    position: absolute;
    right: 15px; top: -20px;
    border: 10px solid transparent;
    border-bottom: 10px solid #22275e;
}
.top-user-box .menu .item{
    display: block;
    border-bottom: 1px solid rgba(255,255,255,0.1);
    padding: 10px 20px;
    position: relative;
    color: #fff;
    text-decoration: none;
}
.top-user-box .menu .item:last-of-type{
    border-bottom: 0;
}


.top-user-box .notify .list{
    display: none;
    position: absolute;
    width: 300px;
    top: 55px;
    right: -5px;
    -webkit-border-radius: 5px;
    -moz-border-radius: 5px;
    border-radius: 5px;
    background: #ffffff;
    z-index: 10;
    -webkit-box-shadow: 0 -5px 6px rgba(0,0,0,0.2);
    -moz-box-shadow:  0 -5px 6px rgba(0,0,0,0.2);
    box-shadow:  0 -5px 6px rgba(0,0,0,0.2);
    padding: 5px 0;
    border: 1px solid #22275e;
}
.top-user-box .notify .list::after {
    content: '';
    position: absolute;
    right: 15px; top: -20px;
    border: 10px solid transparent;
    border-bottom: 10px solid #fff;
}
.top-user-box .notify .list .item{
    display: block;
    border-bottom: 1px solid #22275e;
    padding: 10px 20px;
    position: relative;
    text-decoration: none;
}
.top-user-box .notify .list .item:last-of-type{
    border-bottom: 0;
}

