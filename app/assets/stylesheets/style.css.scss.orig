.news-item{
    position: relative;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    padding: 20px 25px;
    margin-bottom: 21px;
}
.news-item > div{
    position: relative;
    z-index: 10;
}
.news-item:after{
    content: "";
    position: absolute;
    left: -1px;
    top: -1px;
    right: -1px;
    bottom: -1px;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    background: rgb(44,213,210);
    background: -moz-linear-gradient(left,  rgba(44,213,210,1) 0%, rgba(43,50,135,1) 100%);
    background: -webkit-linear-gradient(left,  rgba(44,213,210,1) 0%,rgba(43,50,135,1) 100%);
    background: linear-gradient(to right,  rgba(44,213,210,1) 0%,rgba(43,50,135,1) 100%);
    filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#2cd5d2', endColorstr='#2b3287',GradientType=1 );
    z-index: 1;
}
.news-item:before{
    content: "";
    position: absolute;
    left: 0px;
    top: 0px;
    right: 0px;
    bottom: 0px;
    -webkit-border-radius: 7px;
    -moz-border-radius: 7px;
    border-radius: 7px;
    background: #f6f6ff;
    z-index: 2;
}

