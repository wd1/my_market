.animated {
    animation-duration: 2s;
    animation-fill-mode: both;
    transition-timing-function: linear;
}
.animated.s3 {
    animation-duration: 3s;
    animation-fill-mode: both;
}

.animated.infinite {
    animation-iteration-count: infinite;
}
@keyframes flash {
    from{
        opacity: 0.2;
    }
    15%{
        opacity: 0.5;
    }
    30%{
        opacity: 0.2;
    }
    45%{
        opacity: 0.5;
    }
    60%{
        opacity: 0.2;
    }
}

.flash {
    animation-name: flash;
}

@keyframes fly {
    from{
        top: -15px;
    }
    25%{
        top: -18px;
    }
    75%{
        top: -12px;
    }
    to{
        top: -15px;
    }
}

.fly {
    animation-name: fly;
}

